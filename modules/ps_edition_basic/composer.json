  "description": "PrestaShop Edition Basic module",
  "license": "AFL-3.0",
  "require": {
    "php": ">=7.2.5 <=8.0",
    "prestashop/module-lib-service-container": "^2.0",
    "symfony/string": "<6.0",
    "prestashop/prestashop-accounts-installer": "^1.0",
    "prestashopcorp/module-lib-billing": "^3.3"
  },
  "require-dev": {
